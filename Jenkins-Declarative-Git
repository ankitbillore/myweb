pipeline{
    agent any
    
    environment{
        PATH = "/usr/share/maven/:$PATH"
    }
    stages{
        stage("Git Checkout"){
            steps{
                git credentialsId: 'github', url: 'https://github.com/ankitbillore/myweb'
            }
        }
        stage("Maven Build"){
            steps{
                sh "mvn clean package"
            }
        }
       
            
            
    }
}
